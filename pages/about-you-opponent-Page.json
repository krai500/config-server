    "preferredOptionConfig": {
      "id": "aboutPage.preferredContact",
      "default": "email",
      "options": [
        {
          "label": "allianz-customer.userFormConfig.ac_email",
          "type": "text",
          "value": "email",
          "name": "email",
          "required": true,
          "errorMsg":"about-you.preferredOptionConfig.email.errorMessage",
